{
  "name": "tj-blog-backend",
  "version": "0.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all nodemon",
    "nodemon": "npx nodemon --inspect index | pino-pretty",
    "gcloud:init": "echo \"source ~/google-cloud-sdk/path.bash.inc && source ~/google-cloud-sdk/completion.bash.inc\" >> ~/.bashrc",
    "gcloud:set-project": "node .scripts/set-project | bash",
    "gcloud:login": "npm run gcloud:set-project && ~/google-cloud-sdk/bin/gcloud auth login --no-launch-browser",
    "gcloud:login:adc": "npm run gcloud:set-project && ~/google-cloud-sdk/bin/gcloud auth application-default login --no-launch-browser",
    "gcloud:login:sample": "export CI_COMMIT_BRANCH=sample && npm run gcloud:set-project && ~/google-cloud-sdk/bin/gcloud auth login --no-launch-browser",
    "gcloud:login:adc:sample": "export CI_COMMIT_BRANCH=sample && npm run gcloud:set-project && ~/google-cloud-sdk/bin/gcloud auth application-default login --no-launch-browser",
    "git:ci:login": "git config --global url.\"https://gitlab-ci-token:${CI_JOB_TOKEN}@gitlab.com\".insteadOf https://gitlab.com",
    "set-quota": "node .scripts/set-quota-project | bash",
    "secret:download:sample": "export CI_COMMIT_BRANCH=sample && node .senti/terraform/get-secrets | bash",
    "secret:download": "node .senti/terraform/get-secrets | bash",
    "build": "node .scripts/builds-submit | bash",
    "build:sample": "export CI_COMMIT_BRANCH=sample && node .scripts/builds-submit | bash",
    "update": "npx npm-check-updates",
    "release": "standard-version",
    "push:tags": "git push --follow-tags origin master --no-verify"

  },
  "author": "TJ Monserrat",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/eslint-parser": "7.23.10",
    "@babel/plugin-external-helpers": "7.23.3",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.23.9",
    "@babel/plugin-proposal-object-rest-spread": "7.18.9",
    "@babel/plugin-transform-runtime": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/runtime": "7.23.9",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@microsoft/tsdoc": "0.14.2",
    "@types/mocha": "10.0.6",
    "@types/node": "20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "conventional-changelog": "5.1.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "9.0.10",
    "inquirer": "9.2.14",
    "nodemon": "3.0.3",
    "npm-check-updates": "16.14.14",
    "npm-run-all": "4.1.5",
    "properties": "1.2.1",
    "semistandard": "17.0.0",
    "simple-git": "3.22.0",
    "standard-version": "9.5.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@fastify/cors": "9.0.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/sensible": "5.5.0",
    "@fastify/swagger": "^8.14.0",
    "@google-cloud/logging": "11.0.0",
    "@scalar/fastify-api-reference": "^1.14.8",
    "@sentry/node": "7.100.1",
    "@sentry/tracing": "7.100.1",
    "@zentered/fastify-sentry": "2.2.1",
    "dotenv": "16.4.1",
    "fastify": "4.26.0",
    "fastify-openapi-glue": "^4.4.3",
    "firebase-admin": "12.0.0",
    "google-auth-library": "9.6.3",
    "path-to-regexp": "6.2.1",
    "pino-pretty": "10.3.1"
  }
}
